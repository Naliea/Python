#A list that computes the sum of the items
list_integers=input("Write a list of integers:")
list_numbers=[sum(int(n) for n in list_integers.split())]
print(list_numbers)

#A tuple that prints the value of each item
tuple_books =("Gay Girl Good God","Upon waking","Quest for love","Passion and Purity","Chaos with purpose")
for n in tuple_books:
  print(n)

#Dictionary containing user's information
Name = input("What is your name:")
Age = input("What is your age:")
Favourite_color = input("What is your favourite color:")
user_data = {
  'Name':Name,
  'Age':Age,
  'Favourite_color':Favourite_color,
}
print(user_data)

#Creating sets
input_set_One = input("Enter a set of integers:")
set_One = set(map(int,input_set_One.split()))
input_set_Two = input("Enter another set of integers:")
set_Two = set(map(int,input_set_Two.split()))
New_set = set_One.intersection(set_Two)
print("Common elements in the sets:",New_set)

#Creating a list that as odd no. of characters using list copmmrehesion
list_words = ['Peace','Chiri','Lydia','Marianne','Maji']
new_List = [
word for word in list_words 
  if len(word)%2 != 0]
